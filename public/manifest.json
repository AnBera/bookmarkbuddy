{
  "manifest_version": 2,
  "name": "BookmarkBuddy",
  "author": "Anirban Bera",
  "version": "1.0.1",
  "description": "This extension helps you manage your bookmarks.",
  "permissions": ["bookmarks",
      "chrome://favicon/",
      "storage"
  ],
  "browser_action": {
      "default_title": "BookmarkBuddy",
      "default_popup": "index.html"
  },
  "chrome_url_overrides" : {
      "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
